        "semRepPat": "\\P U Q x. exists y. (P(y) & U(y)(Q)(x))",
        "semSig": {
            "P": "<e,t>",
            "Q": "<e,t>",
            "U": "<e,<<e,t>,<e,t>>>"
        }
    },
