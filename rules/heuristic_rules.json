[
    {
	"explanation": "proper name in subject position",
        "conditions": [
            "tag element {NE}",
            "rel element {SB}"
        ],
        "semRepPat": "\\P. P({[lemma]})",
        "semSig": {
            "P": "<e,t>",
            "{[lemma]}": "e"
        }
    },
    {
	"explanation": "proper name not in subject position",
        "conditions": [
            "tag element {NE}",
            "rel notElement^{NK} {SB}"
        ],
        "semRepPat": "{[lemma]}",
        "semSig": {
            "{[lemma]}": "e"
        }
    },
    {
	"explanation": "normal nouns without dependents (assume plural)",
        "conditions": [
            "tag element {NN}",
            "deps subset {}"
        ],
        "semRepPat": "\\P. all x. ({[lemma]}(x) -> P(x))",
        "semSig": {
            "{[lemma]}": "<e,t>",
            "P": "<e,t>"
        }
    },
    {
	"explanation": "normal nouns with article or modifier",
        "note": "TODO: determine type of noun (with adjective, with article, â€¦",
        "conditions": [
            "tag element {NN}"
        ],
        "semRepPat": "\\x. {[lemma]}(x)",
        "semSig": {
            "{[lemma]}": "<e,t>"
        }
    },
    {
        "explanation": "'ein' in subject.",
        "conditions": [
            "tag element {ART}",
            "lemma element {ein}",
            "rel element^{NK} {SB}"
        ],
        "semRepPat": "\\P Q. exists x. (P(x) & Q(x))",
        "semSig": {
            "P": "<e,t>",
            "Q": "<e,t>"
        }
    },
    {
        "explanation": "'ein' in accusative object of monotransitive verbs and of ditransitive verbs with a second accusative object.",
        "conditions": [
            "tag element {ART}",
            "lemma element {ein}",
            "rel element^{NK} {OA}",
            "! deps superset^{NK, OA} {DA}"
        ],
        "semRepPat": "\\P R x. exists y. (P(y) & R(y)(x))",
        "semSig": {
            "P": "<e,t>",
            "R": "<e,<e,t>>"
        }
    },
    {
        "explanation": "'ein' in accusative object of ditransitive verbs.",
        "conditions": [
            "tag element {ART}",
            "lemma element {ein}",
            "rel element^{NK} {OA}",
            "deps superset^{NK, OA} {DA}"
        ],
        "semRepPat": "\\P T y x. exists z. (P(z) & T(z)(y)(x))",
        "semSig": {
            "P": "<e,t>",
            "T": "<e,<e,<e,t>>>"
        }
    },
    {
        "explanation": "'ein' in dative object of ditransitive verbs.",
        "conditions": [
            "tag element {ART}",
            "lemma element {ein}",
            "rel element^{NK} {DA}"
        ],
        "semRepPat": "\\P R x. exists y. (P(y) & R(y)(x))",
