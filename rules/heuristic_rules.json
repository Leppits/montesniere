    {
	"explanation": "proper name in subject position",
        "conditions": [
            "tag element {NE}",
            "rel element {SB}"
        ],
        "semRepPat": "\\P. P({[lemma]})",
        "semSig": {
            "P": "<e,t>",
            "{[lemma]}": "e"
        }
    },
